<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>

<VBox spacing="20" styleClass="gradient-bg" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.eventmanagement.UploadMaterialController">
    <Label text="Upload Material" styleClass="title-text" />
    <Button fx:id="uploadButton" text="Upload Files" onAction="#handleUpload" styleClass="primary-button" />
    <ListView fx:id="fileListView" styleClass="list-view" />
</VBox>